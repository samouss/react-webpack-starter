{
  "name": "react-webpack-start",
  "version": "1.0.0",
  "description": "Wepack React with Babel, CSS Modules, ESLint, Jest, Enzyme",
  "scripts": {
    "clean": "rm -rf dist",
    "start": "npm run clean && webpack-dev-server --inline --no-info",
    "build": "npm run clean && webpack --env.production",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/samouss/react-webpack-starter.git"
  },
  "author": [
    "Samuel Vaillant <samuel.vllnt@gmail.com>"
  ],
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "7.1.3",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-plugin-react-css-modules": "3.3.0",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.6.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "core-js": "2.5.1",
    "css-loader": "0.28.7",
    "enzyme": "2.9.1",
    "enzyme-to-json": "1.5.1",
    "eslint": "4.6.1",
    "eslint-config-airbnb": "15.1.0",
    "eslint-import-resolver-webpack": "0.8.3",
    "eslint-loader": "1.9.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jest": "21.0.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.3.0",
    "extract-text-webpack-plugin": "3.0.0",
    "html-loader": "0.5.1",
    "html-webpack-plugin": "2.30.1",
    "identity-obj-proxy": "3.0.0",
    "jest": "21.0.1",
    "postcss-loader": "2.0.6",
    "postcss-nested": "2.1.2",
    "react-test-renderer": "15.6.1",
    "style-loader": "0.18.2",
    "webpack": "3.5.6",
    "webpack-dev-server": "2.7.1"
  },
  "dependencies": {
    "prop-types": "15.5.10",
    "react": "15.6.1",
    "react-dom": "15.6.1"
  },
  "browserslist": [
    "last 2 versions",
    "ie >= 10"
  ],
  "jest": {
    "moduleDirectories": [
      "src",
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.css$": "identity-obj-proxy"
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
