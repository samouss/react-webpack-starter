{
  "name": "react-webpack-start",
  "version": "1.0.0",
  "description": "Webpack React Starter with Babel, ESLint, Mocha, Chai, Sinon, Enzyme",
  "scripts": {
    "clean": "rm -rf dist",
    "start": "npm run clean && node_modules/.bin/webpack-dev-server --inline --no-info",
    "build": "npm run clean && node_modules/.bin/webpack --env.production",
    "test": "jest",
    "test:watch": "npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/samouss/react-webpack-starter.git"
  },
  "author": [
    "Samuel Vaillant <samuel.vllnt@gmail.com>"
  ],
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "7.1.3",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-plugin-module-resolver": "2.7.1",
    "babel-plugin-react-css-modules": "3.2.1",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.6.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "core-js": "2.5.1",
    "css-loader": "0.28.7",
    "enzyme": "2.9.1",
    "enzyme-to-json": "1.5.1",
    "eslint": "4.5.0",
    "eslint-config-airbnb": "15.1.0",
    "eslint-import-resolver-babel-module": "3.0.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jest": "20.0.3",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.3.0",
    "extract-text-webpack-plugin": "3.0.0",
    "html-loader": "0.5.1",
    "html-webpack-plugin": "2.30.1",
    "identity-obj-proxy": "3.0.0",
    "jest": "20.0.4",
    "postcss-loader": "2.0.6",
    "postcss-nested": "2.1.2",
    "react-test-renderer": "15.6.1",
    "style-loader": "0.18.2",
    "webpack": "3.5.5",
    "webpack-dev-server": "2.7.1"
  },
  "dependencies": {
    "prop-types": "15.5.10",
    "react": "15.6.1",
    "react-dom": "15.6.1"
  },
  "browserslist": [
    "last 2 versions",
    "ie >= 10"
  ],
  "jest": {
    "moduleNameMapper": {
      "\\.css$": "identity-obj-proxy"
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
