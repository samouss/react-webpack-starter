{
  "name": "react-webpack-start",
  "version": "0.1.0",
  "description": "Webpack React Starter with Babel, ESLint, Mocha, Chai, Sinon, Enzyme",
  "scripts": {
    "clean": "rm -rf dist",
    "start": "npm run clean && node_modules/.bin/webpack-dev-server --inline --hot --no-info",
    "build": "npm run clean && node_modules/.bin/webpack --env.production --progress",
    "test": "npm run mocha \"src/**/*.spec.js\"",
    "mocha": "mocha --require babel-register --require test-setup.js",
    "mocha:watch": "npm run mocha -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/samouss/react-webpack-starter.git"
  },
  "author": [
    "Samuel Vaillant <samuel.vllnt@gmail.com>"
  ],
  "license": "MIT",
  "devDependencies": {
    "babel-core": "6.23.1",
    "babel-loader": "6.3.2",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-react": "6.23.0",
    "babel-register": "6.23.0",
    "chai": "3.5.0",
    "css-loader": "0.26.1",
    "enzyme": "2.7.1",
    "eslint": "3.15.0",
    "eslint-config-airbnb": "14.1.0",
    "eslint-loader": "1.6.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.10.0",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "html-loader": "0.4.4",
    "html-webpack-plugin": "2.28.0",
    "jsdom": "9.11.0",
    "mocha": "3.2.0",
    "react-addons-test-utils": "15.4.2",
    "sinon": "1.17.7",
    "style-loader": "0.13.1",
    "webpack": "2.2.1",
    "webpack-dev-server": "2.3.0"
  },
  "dependencies": {
    "react": "15.4.2",
    "react-dom": "15.4.2"
  }
}
