{
  "name": "react-webpack-start",
  "version": "0.1.0",
  "description": "Webpack React Starter with Babel, ESLint, Mocha, Chai, Sinon, Enzyme",
  "scripts": {
    "start": "webpack-dev-server --inline --hot --content-base dist --no-info --open",
    "build": "rm -rf ./dist && NODE_ENV=production webpack --progress",
    "test": "mocha --require babel-register --require test-setup.js \"./src/**/*.spec.js\"",
    "test:watch": "npm test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/samouss/react-webpack-starter.git"
  },
  "author": [
    "Samuel Vaillant <samuel.vllnt@gmail.com>"
  ],
  "license": "MIT",
  "devDependencies": {
    "babel-core": "6.16.0",
    "babel-loader": "6.2.5",
    "babel-plugin-transform-object-rest-spread": "6.16.0",
    "babel-preset-es2015": "6.16.0",
    "babel-preset-react": "6.16.0",
    "babel-register": "6.16.3",
    "chai": "3.5.0",
    "css-loader": "0.25.0",
    "enzyme": "2.4.1",
    "eslint": "3.6.1",
    "eslint-config-airbnb": "12.0.0",
    "eslint-loader": "1.5.0",
    "eslint-plugin-import": "1.16.0",
    "eslint-plugin-jsx-a11y": "2.2.2",
    "eslint-plugin-react": "6.3.0",
    "extract-text-webpack-plugin": "1.0.1",
    "html-loader": "0.4.4",
    "html-webpack-plugin": "2.22.0",
    "jsdom": "9.5.0",
    "json-loader": "0.5.4",
    "mocha": "3.1.0",
    "react-addons-test-utils": "15.3.2",
    "sinon": "1.17.6",
    "style-loader": "0.13.1",
    "webpack": "1.13.2",
    "webpack-dev-server": "1.16.1"
  },
  "dependencies": {
    "react": "15.3.2",
    "react-dom": "15.3.2"
  }
}
